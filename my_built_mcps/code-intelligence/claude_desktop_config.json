      }
    },
    "desktop-commander": {
      "command": "npx.cmd",
      "args": [
        "@wonderwhy-er/desktop-commander@latest"
      ]
    },
    "supabase": {
      "command": "npx",
      "args": [
        "-y",
        "@supabase/mcp-server-supabase@latest",
        "--access-token",
        "<personal-access-token>"
      ]
    },
